{
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "parser":"typescript",
  "tabWidth": 2,
  "semi": true,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "parser": "markdown"
      }
    },
    {
      "files": "*.feature",
      "options": {
        "parser": "cucumber"
      }
    }
  ]
}
